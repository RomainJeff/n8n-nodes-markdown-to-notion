{
	"name": "n8n-nodes-markdown-to-notion",
	"version": "0.1.0",
	"description": "A node that converts Markdown to Notion blocks using the Martian library",
	"license": "MIT",
	"homepage": "https://github.com/RomainJeff/n8n-nodes-markdown-to-notion",
	"keywords": [
		"n8n-community-node-package"
	],
	"author": {
		"name": "Romain Quilliot",
		"email": "hello@romainjeff.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/RomainJeff/n8n-nodes-markdown-to-notion.git"
	},
	"scripts": {
		"build": "node build.js",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"release": "n8n-node release",
		"prepublishOnly": "npm run build && n8n-node prerelease"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"strict": false,
		"nodes": [
			"dist/nodes/MarkdownToNotion/MarkdownToNotion.node.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "*",
		"@tryfabric/martian": "^1.2.4",
		"esbuild": "^0.25.11",
		"eslint": "9.32.0",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"typescript": "5.9.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
